name: pxlfall's override for basic WF API
desc: Last update on 24.10.21 04:21

# 1. No GEOSITE anymore!
# 2. I wonder if we can turn it into mixin for CFW tho...
# 3. Changed DNS again, extensive testing!

port: 7890 #!replace
socks-port: 7891 #!replace
redir-port: 7892 #!replace
mixed-port: 7893 #!replace
allow-lan: false #!replace
mode: rule #!replace
log-level: warning #!replace
ipv6: true #!replace
script: #!replace
  shortcuts:
    udp-relay-disable: network == 'udp'
hosts: #!replace
  '*.clash.dev': 127.0.0.1
  'alpha.clash.dev': '::1'
  'metrics-picture.d.meituan.net': 103.37.152.48
  'maplocatesdksnapshot.d.meituan.net': 103.37.152.48
  'route-stats.d.meituan.net': 101.236.69.94
external-controller: 0.0.0.0:9090 #!replace
dns: #!replace
  enable: true
  ipv6: true
  listen: 0.0.0.0:53
  default-nameserver:
    - 114.114.114.114
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  fake-ip-filter:
    - '*.lan'
    - 'stun.*.*.*'
    - 'stun.*.*'
    - 'time.windows.com'
    - 'time.nist.gov'
    - 'time.apple.com'
    - 'time.asia.apple.com'
    - '*.ntp.org.cn'
    - '*.openwrt.pool.ntp.org'
